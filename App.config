<?xml version="1.0" encoding="utf-8"?>
<configuration>
<appSettings>
	
	<!-- max thread throttle -->
	<add key="MaxConcurrentThreads" value="5"/>
	<!-- delay in milliseconds -->
    <add key="QueueSleepWhenEmpty" value="500"/>
    <!-- db connection timeout in seconds -->
    <add key="DBConnectRetry" value="60"/>
    <!-- the queue server path -->
    <add key="QueueServer" value="DOLWBOLYVSDEV1"/>
    <!-- the input queue -->
    <add key="QueueRx" value="PRIVATE$\Vessel"/>
    <!-- the output queue -->
    <add key="QueueTx" value="PRIVATE$\DBQTx"/>
    <!-- debug mode 0 = normal, 1 = log messages, 2 = log everything -->
    <add key="DebugMode" value="2"/>
    <!-- proxy -->
    <add key="Proxy" value=""/>
    <!-- user -->
    <add key="User" value=""/>
    <!-- password -->
    <add key="Password" value=""/>
    <!-- domain -->
    <add key="Domain" value=""/>
    <!-- UseSystemCredentials -->
    <add key="UseSystemCredentials" value="True"/>
    <!-- PreAuthenticate -->
    <add key="PreAuthenticate" value="False"/>
    
    <!-- the URL of the Fujitsu web service (this needs to be modified when	moving into production -->
    <add key="vsdWSPVesselInquiry.dolwebservice.CallMVBWSP" value="http://dolservicedev/dolwebservice.asmx"/>
    
    <!-- QA http://dolserviceqa/dolwebservice.asmx -->
	<!-- DEV http://dolservicedev/dolwebservice.asmx -->
    
    </appSettings>
    
</configuration>
